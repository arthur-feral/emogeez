{
  "name": "emogeez-generator",
  "version": "0.19.5",
  "description": "Build an emojis sprite and stylesheet file from emojis datas collected from both emojipedia.org and unicode.org website",
  "scripts": {
    "start": "TEMP_FILES_PATH=tmp node ./dist/index.js",
    "dev": "node ./index.js",
    "prepublish": "echo \"run tests?\"",
    "test:js": "NODE_ENV=test TEMP_FILES_PATH=tests/tmp mocha -r @babel/register -r @babel/polyfill --timeout 5000 -r tests/bootstrap.js $(find lib -name '*.test.js')",
    "lint:js": "eslint ./ --ext .js --ignore-path ./.eslintignore --fix",
    "test": "yarn run lint:js && yarn run test:js",
    "clean": "rm -r tmp/**/*"
  },
  "homepage": "https://github.com/arthur-feral/emogeez/tree/master/packages/emogeez-generator",
  "keywords": [
    "emoji",
    "emojis",
    "generator",
    "sprite",
    "sprites",
    "svg",
    "png",
    "jpeg",
    "jpg"
  ],
  "author": "Arthur FÃ©ral, iAdvize<arthur.feral@hotmail.fr>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "eventemitter3": "^3.0.1",
    "fs-extra": "^1.0.0",
    "gm": "^1.23.0",
    "image-size": "^0.5.0",
    "jimp": "^0.2.27",
    "lodash": "^4.17.5",
    "pngquant": "^1.3.0",
    "spritesmith": "^3.3.0",
    "superagent": "3.8.2",
    "superagent-throttle": "0.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-inline-import": "^3.0.0",
    "chai": "^3.5.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "sinon": "^1.17.6",
    "superagent-mock": "3.7.0"
  }
}
