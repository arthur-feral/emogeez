{
  "name": "emogeez-generator",
  "version": "0.8.2",
  "description": "Build an emojis sprite and stylesheet file from emojis datas collected from both emojipedia.org and unicode.org website",
  "scripts": {
    "start": "TEMP_FILES_PATH=tmp node ./dist/index.js",
    "dev": "node ./index.js",
    "transpile": "$(npm bin)/babel . --out-dir dist --ignore cache,tests,node_modules,nyc-reports,dist,*.test.js",
    "prepublish": "yarn run test",
    "postinstall": "yarn run transpile",
    "lint:js": "eslint lib tests --ext .js",
    "test": "NODE_ENV=test TEMP_FILES_PATH=tests/tmp nyc --check-coverage --lines 85 --report-dir=${CIRCLE_ARTIFACTS:-'./nyc-reports'} --reporter=lcov yarn run mocha",
    "mocha": "NODE_ENV=test TEMP_FILES_PATH=tests/tmp mocha --compilers js:babel-register -r tests/bootstrap.js -r babel-polyfill $(find lib -name '*.test.js')",
    "clean": "rm -r tmp/**/*"
  },
  "engines": {
    "node": ">=9.2.0"
  },
  "homepage": "https://github.com/arthur-feral/emogeez/tree/master/packages/emogeez-generator",
  "keywords": [
    "emoji",
    "emojis",
    "generator",
    "sprite",
    "sprites",
    "svg",
    "png",
    "jpeg",
    "jpg"
  ],
  "author": "Arthur FÃ©ral, iAdvize<arthur.feral@hotmail.fr>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "eventemitter3": "^3.0.1",
    "fs-extra": "^1.0.0",
    "gm": "^1.23.0",
    "image-size": "^0.5.0",
    "jimp": "^0.2.27",
    "lodash": "^4.17.5",
    "pngquant": "^1.3.0",
    "spritesmith": "^3.3.0",
    "superagent": "3.8.2",
    "superagent-throttle": "0.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-env": "^2.4.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^4.14.0",
    "mocha": "^3.2.0",
    "nyc": "^11.3.0",
    "sinon": "^1.17.6",
    "superagent-mock": "3.7.0"
  },
  "nyc": {
    "exclude": [
      "dist/",
      "tests/"
    ]
  }
}
