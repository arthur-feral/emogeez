{
  "name": "emogeez-react-components",
  "version": "0.12.0",
  "description": "A toolbox for emojis rendering.",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "$(npm bin)/babel . --out-dir dist --ignore cache,tests,node_modules,nyc-reports,dist,*.test.js",
    "prepublish": "yarn run test",
    "postinstall": "yarn run transpile",
    "lint:js": "eslint lib tests --ext .js",
    "test": "NODE_ENV=test nyc --check-coverage --lines 85 --report-dir=${CIRCLE_ARTIFACTS:-'./nyc-reports'} --reporter=lcov yarn run mocha",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-register -r tests/bootstrap.js -r babel-polyfill $(find src -name '*.test.js')",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": ">=9.2.0"
  },
  "homepage": "https://github.com/arthur-feral/emogeez/tree/master/packages/emogeez-react-components",
  "keywords": [
    "emoji",
    "emojis",
    "parser",
    "parsing",
    "react"
  ],
  "author": "Arthur FÃ©ral, iAdvize<arthur.feral@hotmail.fr>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-backgrounds": "^3.3.6",
    "@storybook/addon-centered": "^3.3.6",
    "@storybook/addon-knobs": "^3.3.6",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "emogeez-generator": "^0.11.2",
    "emogeez-parser": "^0.11.1",
    "eslint": "^4.14.0",
    "jsdom": "^11.7.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.8.3",
    "nyc": "^11.3.0",
    "sinon": "4.5.0"
  },
  "nyc": {
    "exclude": [
      "dist/",
      "tests/"
    ]
  }
}
