{
  "parser": "babel-eslint",
  "extends": [
    "iadvize"
  ],
  "rules": {
    "global-require": "warn"
  },
  "overrides": [
    {
      "files": [
        "**/*.test.js"
      ],
      "globals": {
        "describe": true,
        "it": true,
        "beforeEach": true,
        "afterEach": true,
        "before": true,
        "after": true
      }
    }
  ]
}
